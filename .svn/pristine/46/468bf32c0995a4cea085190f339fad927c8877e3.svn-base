package com.std.igek.entity;

import java.io.Serializable;

public class Goods implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -7202238214890214951L;
	
	private int gid;
	private String gname;
	private String gshow;
	private String url;
	private double price;
	private double tex;
	private int gclass;
	private int country;
	private String wl;
	private int amount;
	private int ishot;
	private String country_name;
	
	public int getGid() {
		return gid;
	}
	public void setGid(int gid) {
		this.gid = gid;
	}
	public String getGname() {
		return gname;
	}
	public void setGname(String gname) {
		this.gname = gname;
	}
	public String getGshow() {
		return gshow;
	}
	public void setGshow(String gshow) {
		this.gshow = gshow;
	}
	public String getUrl() {
		return url;
	}
	public void setUrl(String url) {
		this.url = url;
	}
	public double getPrice() {
		return price;
	}
	public void setPrice(double price) {
		this.price = price;
	}
	public double getTex() {
		return tex;
	}
	public void setTex(double tex) {
		this.tex = tex;
	}
	public int getGclass() {
		return gclass;
	}
	public void setGclass(int gclass) {
		this.gclass = gclass;
	}
	
	public int getCountry() {
		return country;
	}
	public void setCountry(int country) {
		this.country = country;
	}
	public String getWl() {
		return wl;
	}
	public void setWl(String wl) {
		this.wl = wl;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public int getIshot() {
		return ishot;
	}
	public void setIshot(int ishot) {
		this.ishot = ishot;
	}
	
	
	public String getCountry_name() {
		return country_name;
	}
	public void setCountry_name(String country_name) {
		this.country_name = country_name;
	}
	@Override
	public String toString() {
		return "Goods [gid=" + gid + ", gname=" + gname + ", gshow=" + gshow + ", url=" + url + ", price=" + price
				+ ", tex=" + tex + ", gclass=" + gclass + ", country=" + country + ", wl=" + wl + ", amount=" + amount
				+ ", ishot=" + ishot + ", country_name=" + country_name + "]";
	}
	
	
	
	
	
}
